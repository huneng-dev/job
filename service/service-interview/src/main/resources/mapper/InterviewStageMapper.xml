<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.hjf.job.interview.mapper.InterviewStageMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="cn.hjf.job.interview.entity.InterviewStage">
                    <result column="record_id" property="recordId" />
                    <result column="stage_id" property="stageId" />
                    <result column="start_time" property="startTime" />
                    <result column="interview_location" property="interviewLocation" />
                    <result column="status" property="status" />
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="columns">
                record_id, stage_id, start_time, interview_location, status
        </sql>

</mapper>
