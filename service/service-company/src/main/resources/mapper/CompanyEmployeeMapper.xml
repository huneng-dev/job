<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.hjf.job.company.mapper.CompanyEmployeeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.hjf.job.model.entity.company.CompanyEmployee">
        <result column="user_id" property="userId"/>
        <result column="company_id" property="companyId"/>
        <result column="title_id" property="titleId"/>
        <result column="is_admin" property="isAdmin"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="columns">
        user_id
        ,company_id,title_id,is_admin
    </sql>

</mapper>