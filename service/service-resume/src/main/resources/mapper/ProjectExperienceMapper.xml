<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.hjf.job.resume.mapper.ProjectExperienceMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="cn.hjf.job.resume.entity.ProjectExperience">
                    <result column="resume_id" property="resumeId" />
                    <result column="project_name" property="projectName" />
                    <result column="role" property="role" />
                    <result column="start_time" property="startTime" />
                    <result column="end_time" property="endTime" />
                    <result column="project_description" property="projectDescription" />
                    <result column="project_url" property="projectUrl" />
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="columns">
                resume_id, project_name, role, start_time, end_time, project_description, project_url
        </sql>

</mapper>
