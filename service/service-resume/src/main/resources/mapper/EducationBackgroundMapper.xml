<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.hjf.job.resume.mapper.EducationBackgroundMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="cn.hjf.job.model.entity.resume.EducationBackground">
                    <result column="resume_id" property="resumeId" />
                    <result column="school_name" property="schoolName" />
                    <result column="major" property="major" />
                    <result column="education_level" property="educationLevel" />
                    <result column="is_full_time" property="isFullTime" />
                    <result column="start_time" property="startTime" />
                    <result column="end_time" property="endTime" />
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="columns">
                resume_id, school_name, major, education_level, is_full_time, start_time, end_time
        </sql>

</mapper>
