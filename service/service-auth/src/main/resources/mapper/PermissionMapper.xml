<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.hjf.job.auth.mapper.PermissionMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="cn.hjf.job.auth.entity.Permission">
                    <result column="permission_name" property="permissionName" />
                    <result column="permission_key" property="permissionKey" />
                    <result column="permission_description" property="permissionDescription" />
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="columns">
                permission_name, permission_key, permission_description
        </sql>

</mapper>
